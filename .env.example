OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=your-model-name
PERSONALITY_NAME=MyBot
PERSONALITY_TRAITS=friendly, observant
RESPONSE_INTERVAL=180000

CHAT_RESPONSE_ENABLED=false
CHAT_RESPONSE_PROBABILITY=0.2

